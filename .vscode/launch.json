{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker: Nodemon Debug",
      "type": "node",
      "request": "attach",
      "restart": true,
      "protocol": "inspector",
      "port": 9229,
      "address": "localhost",
      "localRoot": "${workspaceFolder}/backend",
      "remoteRoot": "/usr/src/app",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true
    }
  ]
}
