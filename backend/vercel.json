{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "authRoutes.js",
      "use": "@vercel/node"
    },
    {
      "src": "emailRoutes.js",
      "use": "@vercel/node"
    },
    {
      "src": "./database/database.js",
      "use": "@vercel/node"
    },
    {
      "src": "/sql/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}